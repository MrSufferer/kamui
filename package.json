  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mangekyou-labs/kamui.git"
  },
  "bugs": {
    "url": "https://github.com/mangekyou-labs/kamui/issues"
  },
  "homepage": "https://github.com/mangekyou-labs/kamui#readme"
